*/
if(instance_exists(player) || instance_exists(player2))
setS -= 1;
alarm[0] = room_speed*1;
#define Step_0
/*"/*'/**//* YYD ACTION
lib_id=1
action_id=603
applies_to=self
*/
if(setS < 0){
   if(setMin > 0){
   setS += 60;
   setMin -= 1;
   }else{
      if(setH > 0){
         setS += 60;
         setMin += 59;
         setH -= 1;
      }else{
         if(ros == 0){
            if(global.grav=0){
                with(player){killPlayer();}
                }else{
                with(player2){killPlayer();}
                }
            with(timelimitobject){
            instance_destroy();
            }
         }
         if(ros == 1){
            instance_create(x,y,shake);
            instance_destroy();
         }
         if(ros == 2){
            instance_destroy();
         }
         if(ros >= 3){
            room_goto(ros);

            if (warpX!=0 || warpY!=0) {
                if (!instance_exists(player)) {
                instance_create(warpX, warpY, player);
                }
                player.x = warpX;
                player.y = warpY;
            } else {
                with (player) {
                instance_destroy();
                }
            }
        }
      }
   }
}
